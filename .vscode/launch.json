    {
      "name": "MciroMain",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/ts-node/dist/bin.js",
      "runtimeArgs": ["--inspect"],
      "args": ["${workspaceFolder}/microservices/main/src/server.ts"],
      "env": {
        "NODE_ENV": "development"
      },
      "envFile": "${workspaceFolder}/microservices/main/.env",
