{
  "extends": "@repo/tsconfig-frontend/tsconfig.json",
  "include": ["next-env.d.ts", "next.config.mjs", ".eslintrc.cjs", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/_document.js", "app/_document.js"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
