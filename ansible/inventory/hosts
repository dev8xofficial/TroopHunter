[connect_ops]
connect-ops ansible_port=22 ansible_host=192.168.1.200 ansible_user=connect-ops ansible_ssh_private_key_file=~/.ssh/id_rsa_connect_ops

[docker]
docker-registry ansible_port=22 ansible_host=192.168.1.201 ansible_user=docker-registry ansible_ssh_private_key_file=~/.ssh/id_rsa_registry

[scraper]
scraper_host ansible_port=22 ansible_host=192.168.1.202 ansible_user=ubuntu-server ansible_ssh_private_key_file=~/.ssh/id_ed25519_ubuntu

[k8s_controller]
k8s-ctrlr ansible_port=22 ansible_host=192.168.1.205 ansible_user=k8s-ctrlr ansible_ssh_private_key_file=~/.ssh/id_rsa_k8s_ctrlr

[k8s_workers]
k8s-node-1 ansible_port=22 ansible_host=192.168.1.206 ansible_user=k8s-node-1 ansible_ssh_private_key_file=~/.ssh/id_rsa_k8s_node

[all:vars]
ansible_ssh_common_args='-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null'
